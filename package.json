{
  "name": "godev.space",
  "version": "1.0.4",
  "description": "Web application for Godev.space",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "heroku-nextjs-build",
    "build": "next build",
    "start": "next start -p $PORT",
    "dev": "next",
    "test": "xo"
  },
  "xo": {
    "space": true,
    "esnext": true,
    "semicolon": false,
    "rules": {
      "no-unused-vars": 0
    },
    "ignores": [
      "static/analytics.js"
    ]
  },
  "engines": {
    "node": ">=6.9"
  },
  "cacheDirectories": [
    "nextjs/node_modules"
  ],
  "repository": {
    "url": "git@github.com:LasaleFamine/godev.space.git",
    "type": "git"
  },
  "author": "LasaleFamine <info@godev.space>",
  "license": "MIT",
  "dependencies": {
    "@mars/heroku-nextjs-build": "2.0.0",
    "next": "4.2.1",
    "react": "16.2.0",
    "react-dom": "16.2.0"
  },
  "devDependencies": {
    "xo": "0.18.2"
  }
}
